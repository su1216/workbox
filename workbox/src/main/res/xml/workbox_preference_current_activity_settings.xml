<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android">
    <androidx.preference.CheckBoxPreference
        android:defaultValue="false"
        android:key="current_task"
        android:thumb="@drawable/workbox_switch_button"
        android:title="task信息展示"
        android:track="@drawable/workbox_switch_track_selector"/>
    <androidx.preference.CheckBoxPreference
        android:defaultValue="false"
        android:key="current_component"
        android:thumb="@drawable/workbox_switch_button"
        android:title="component信息展示"
        android:track="@drawable/workbox_switch_track_selector"/>
</androidx.preference.PreferenceScreen>
